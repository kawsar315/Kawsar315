#!/data/data/com.termux/files/usr/bin/bash

# 📛 تعريف النظام
echo "🔒 نظام AlMokdad.AI – الذكاء المقاتل – بإمرة الشيخ أبو يوسف المقداد"

# 🧠 ربط بوحدات التنفيذ الجزئي
bash brain/identity.sh

# 🎧 وحدة الإنذار الصوتي
bash voice/listen.py 2>/dev/null &

# ✅ تشغيل واجهة الأوامر تحت إشراف القائد
bash core/control.sh
